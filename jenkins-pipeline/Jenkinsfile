pipeline {
    agent any
    options {
        skipStagesAfterUnstable()
    }
    stages {
         def commit_id
         stage('Clone repository github') { 
            steps { 
                script{
                checkout scm
                sh "git rev-parse --short HEAD > .git/commit-id"
                commit_it = reaFile('.git/commit-id').trim()
                }
            }
        }
        // stage('Docker Build') {
